import numpy as np
state_size = 4
action_size = 2
state = np.random.rand(state_size)
W_shared = np.random.rand(state_size, 8)
W_value = np.random.rand(8, 1)
W_adv = np.random.rand(8, action_size)
hidden = state @ W_shared
value = hidden @ W_value
advantage = hidden @ W_adv
Q_values = value + (advantage - advantage.mean())
print("State:", state)
print("Dueling DQN Q-values:")
print(Q_values)
